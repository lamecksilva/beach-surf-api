{
  "name": "beach-surf-api",
  "version": "1.0.0",
  "description": "Api de gente grande",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "ts-node-dev 'src/index.ts' --files | pino-pretty -c",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test": "yarn test:unit && yarn test:functional",
    "test:unit": "jest",
    "test:functional": "jest --projects ./test --runInBand --detectOpenHandles"
  },
  "repository": "git@github.com:lamecksilva/beach-surf-api.git",
  "author": "Lameck Sandro <lameck2@hotmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.38",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.12",
    "@types/express-pino-logger": "^4.0.2",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^26.0.23",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/nock": "^11.1.0",
    "@types/node": "^15.12.4",
    "@types/pino": "^6.3.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.29.0",
    "jest": "^27.0.4",
    "nock": "^13.1.1",
    "pino-pretty": "^5.1.2",
    "prettier": "^2.3.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/jsonwebtoken": "^8.5.4",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.13.2",
    "pino": "^6.13.0"
  }
}
